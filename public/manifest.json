{
  "manifest_version": 2,
  "name": "Please Note!",
  "author": "Jacek Budzynski",
  "version": "1.0.0",
  "description": "Make notes from within your browser",
  "icons": {
    "16": "16v2.png",
    "32": "32v2.png",
    "48": "48v2.png",
    "128": "128v2.png"
  },
  "permissions": ["identity", "identity.email"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Please Note!"
  }
}
